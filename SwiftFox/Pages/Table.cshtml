@page "/table/{schema:alpha}.{table:alpha}"
@model SwiftFox.Pages.TableModel
@{
    ViewBag._Title = $"{Model.Schema}.{Model.Table}";
}

<script type="module">
    import  { App } from 'swift-fox/table';
    import { html } from 'sinuous';

    document.body.append(
      html`
        <${App} />
      `
    );
</script>